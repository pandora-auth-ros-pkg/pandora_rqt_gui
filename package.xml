<?xml version="1.0"?>
<package>
  <name>pandora_rqt_gui</name>
  <version>0.0.0</version>
  <description>Rqt gui implementation for remote introspection of PANDORA UGV.</description>

  <maintainer email="chamzask@gmail.com">Chamzas Konstantinos</maintainer>

  <license>BSD</license>

  <author>Chamzas Konstantinos</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roslint</build_depend>
  <run_depend>roslint</run_depend>
  
  <run_depend>rospy</run_depend>
  <run_depend>rqt_gui</run_depend>
  <run_depend>rqt_gui_py</run_depend>
  
  <build_depend>message_generation</build_depend>
  <run_depend>message_generation</run_depend>
  <run_depend>message_runtime</run_depend>
   
  <build_depend>std_msgs</build_depend>
  <run_depend>std_msgs</run_depend>
  
  <build_depend>actionlib_msgs</build_depend>
  <run_depend>actionlib_msgs</run_depend>

  <run_depend>pandora_data_fusion_msgs</run_depend>
  <run_depend>pandora_arm_hardware_interface</run_depend>
  <run_depend>pandora_xmega_hardware_interface</run_depend>
  <run_depend>state_manager</run_depend>
  <run_depend>state_manager_msgs</run_depend>

  
  <export>
     <rqt_gui plugin="${prefix}/plugin.xml"/>

  </export>
</package>
